{
    "version": 2,
    "builds": [
        {
            "src": "registration/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxambda": "15mb", "runtime":"python3.9.2"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "disDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest":"registration/wsgi.py"
        }
    ]
}